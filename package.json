{
  "name": "pipespawn",
  "version": "1.2.0",
  "description": "A utility to pipe a Readable through an external process and return the result as a new Readable.",
  "main": "dist/pipespawn.js",
  "types": "dist/pipespawn.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18.1.0",
    "npm": ">=8.8.0"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:compile": "tsc",
    "build": "npm-run-all build:clean build:compile",
    "lint:fix": "ts-standard --fix",
    "lint": "ts-standard",
    "test:run": "ts-node-esm src/pipespawn.test.ts",
    "test": "npm-run-all lint test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedrhino/node-pipespawn.git"
  },
  "keywords": [
    "pipe",
    "subprocess",
    "readable",
    "stream"
  ],
  "author": "Rhino Maximus",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/wingedrhino/node-pipespawn/issues"
  },
  "homepage": "https://github.com/wingedrhino/node-pipespawn#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "rhinoerror": "^0.1.0"
  }
}
